package com.imooc;

public class Helloworld {
	public static void main(String[] args){
		int count = 0;
		int score = 56;
		System.out.println("加分前的成绩是："+score);
	while(score<70){
		count++;
		score++;
	}
		System.out.println("加分后的成绩是："+score);
		System.out.println("一共加了"+count+"次");

        
	
		
 }
}